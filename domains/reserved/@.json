{
    "repo": "https://github.com/ML-comm/ddns",

    "owner": {
        "username": "ml-comm",
        "email": "admin@mlcommunity.dev"
    },

    "target": {
        "MX": {
            "name": "@",
            "value": "mail.mlcommunity.dev.",
            "priority": 0
        },
        "TXT": [
            {
                "name": "dkim._domainkey",
                "value": "v=DKIM1;k=rsa;t=s;s=email;"
            },
            {
                "name": "@",
                "value": "v=spf1 +a +mx -all"
            },
            {
                "name": "@",
                "value": "v=DMARC1; ruf=mailto:postmaster@mlcommunity.dev"
            }
        ],
        "SRV": {
            "name": "_autodiscover._tcp",
            "priority": 0,
            "weight": 0,
            "port": 443,
            "value": "mail.mlcommunity.dev."
        }
    },

    "proxied": false
}
